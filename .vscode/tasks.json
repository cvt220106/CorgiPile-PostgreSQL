{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"command": "sh",
	"args": ["-c"],
	"presentation": {
		"reveal": "always"
	},
	"tasks": [
		{
			"label": "Configure",
			"type": "shell",
			"command": "./configure --prefix=/home/jacky/postgres-sgd --enable-depend --enable-cassert --enable-debug --with-python CFLAGS='-O3 -Wall -lpthread'",
			"problemMatcher": [
				"$eslint-compact"
			]
		},
		{
			"label": "Make All",
			"type": "shell",
			"command": "make -j4 all",
			"problemMatcher": [
				"$eslint-compact"
			]
		},
		{
			"label": "Make Clean All",
			"type": "shell",
			"command": "make clean",
			"problemMatcher": [
				"$eslint-compact"
			]
		},
		{
			"label": "Make Install",
			"type": "shell",
			"command": "make install"
		},
		{
			"label": "Make Uninstall",
			"type": "shell",
			"command": "make uninstall"
		}
	]
}